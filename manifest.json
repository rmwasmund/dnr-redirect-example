{
  "manifest_version": 3,
  "name": "dnr-redirect-extension",
  "description": "minimal example to demonstrate dnr redirect discrepancies between browsers",
  "version": "1.0",
  "background": {
    "service_worker": "/background.js"
  },
  "permissions": [
    "declarativeNetRequestWithHostAccess"
  ],
  "web_accessible_resources": [
    {
      "resources": ["/web_accessible_resources/noop-1s.mp4"],
      "matches": ["<all_urls>"],
      "use_dynamic_url": true
    }
  ],
  "host_permissions": [
    "<all_urls>"
  ]
}